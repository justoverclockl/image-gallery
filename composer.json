{
  "name": "justoverclock/image-gallery",
  "description": "Add an image gallery to Flarum directly from Flickr",
  "keywords": [
    "flickr gallery",
      "gallery", "flickr", "image gallery"
  ],
  "type": "flarum-extension",
  "license": "MIT",
  "require": {
    "flarum/core": "^1.0"
  },
  "require-dev": {
    "flarum/testing": "^1.0"
  },
  "authors": [
    {
      "name": "Marco Colia",
      "email": "info@flarum.it",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Justoverclock\\ImageGallery\\": "src/"
    }
  },
  "extra": {
    "flarum-extension": {
      "title": "ImageGallery",
      "category": "",
      "icon": {
        "name": "fas fa-camera-retro",
        "backgroundColor": "blue",
        "color": "white"
      }
    },
    "flagrow": {
      "discuss": ""
    }
  }
}
